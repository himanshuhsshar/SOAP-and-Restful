<webServices
    xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://www.religare.com/insurance/service"
    typeNamespaceBase="http://www.religare.com/insurance/types"
    urlPatternBase="/">

    <endpoint
        name="PolicyManager"
        displayName="Policy Manager Service"
        description="Policy Manager web service" 
        wsdl="/WEB-INF/PolicyManager.wsdl"
        interface="com.religareinsurance.service.PolicyManager"  
        implementation="com.religareinsurance.service.PolicyManagerImpl" 
	     model="/WEB-INF/model-rpc-enc.xml.gz">
	</endpoint>
 
    <endpointMapping
        endpointName="PolicyManager"
        urlPattern="/policyManager"/>

</webServices>
