<webServices
    xmlns="http://java.sun.com/xml/ns/jax-rpc/ri/dd"
    version="1.0"
    targetNamespaceBase="http://www.railyatra.com/enquiry/service"
    typeNamespaceBase="http://www.railyatra.com/enquiry/types"
    urlPatternBase="/">

    <endpoint
        name="TrainEnquiry"
        displayName="Train Enquiry Service"
        description="Train Enquiry web service" 
        wsdl="/WEB-INF/RailYatraService.wsdl"
        interface="com.railyatra.services.TrainEnquiry"  
        implementation="com.railyatra.services.TrainEnquiryImpl" 
	     model="/WEB-INF/model-rpc-enc.xml.gz">
	</endpoint>
 
    <endpointMapping
        endpointName="TrainEnquiry"
        urlPattern="/trainEnquiry"/>

</webServices>
